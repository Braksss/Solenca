#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/benji/Desktop/Solenca/solenca/node_modules/.pnpm/json-schema-to-typescript@15.0.3/node_modules/json-schema-to-typescript/dist/src/node_modules:/mnt/c/Users/benji/Desktop/Solenca/solenca/node_modules/.pnpm/json-schema-to-typescript@15.0.3/node_modules/json-schema-to-typescript/dist/node_modules:/mnt/c/Users/benji/Desktop/Solenca/solenca/node_modules/.pnpm/json-schema-to-typescript@15.0.3/node_modules/json-schema-to-typescript/node_modules:/mnt/c/Users/benji/Desktop/Solenca/solenca/node_modules/.pnpm/json-schema-to-typescript@15.0.3/node_modules:/mnt/c/Users/benji/Desktop/Solenca/solenca/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/benji/Desktop/Solenca/solenca/node_modules/.pnpm/json-schema-to-typescript@15.0.3/node_modules/json-schema-to-typescript/dist/src/node_modules:/mnt/c/Users/benji/Desktop/Solenca/solenca/node_modules/.pnpm/json-schema-to-typescript@15.0.3/node_modules/json-schema-to-typescript/dist/node_modules:/mnt/c/Users/benji/Desktop/Solenca/solenca/node_modules/.pnpm/json-schema-to-typescript@15.0.3/node_modules/json-schema-to-typescript/node_modules:/mnt/c/Users/benji/Desktop/Solenca/solenca/node_modules/.pnpm/json-schema-to-typescript@15.0.3/node_modules:/mnt/c/Users/benji/Desktop/Solenca/solenca/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../json-schema-to-typescript@15.0.3/node_modules/json-schema-to-typescript/dist/src/cli.js" "$@"
else
  exec node  "$basedir/../../../../../json-schema-to-typescript@15.0.3/node_modules/json-schema-to-typescript/dist/src/cli.js" "$@"
fi
